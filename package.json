    "babel-preset-react": "^6.24.1",
    "google-translate": "^2.1.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "translate": "^1.0.2",
    "webpack-dev-server": "^3.1.3"
  },
  "devDependencies": {
    "json-loader": "^0.5.7",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
